<h1>リストを編集</h1>
<%= form_with model: @list, url: board_list_path(@board.id, @list.id), method: :patch, local: true do |f| %>
  <%= f.label :content, "リスト名" %>
  <%= f.text_field :content %>
  <%= f.submit "変更", class: 'btn' %>
<% end %>
<%= button_to "削除", board_list_path(@board.id, @list.id), { method: :delete, form: { data: { confirm: "本当に削除しますか？" } } } %>
